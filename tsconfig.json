{
    "compilerOptions": {
        "target": "es2019",
        "module": "commonjs",
        "outDir": "dist",
        "strict": true,
        "strictNullChecks": false,
        "strictPropertyInitialization": false,
        "suppressImplicitAnyIndexErrors": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "declaration": true
    },
    "include": [
        "src/**/*"
    ]
}
